{
  "main": "launcher.html",
  "name": "SmartDashboard.js",
  "description": "Unofficial FRC SmartDashboard in nw.js",
  "version": "0.1.12a",
  "keywords": [
    "frc",
    "smartdashboard"
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/erikuhlmann/SmartDashboard.js"
    }
  ],
  "window": {
    "title": "SmartDashboard.js",
    "icon": "assets/favicon.png",
    "frame": false,
    "width": 300,
    "height": 40,
    "position": "center",
    "transparent": false,
    "show": false,
    "resizable": false
  },
  "js-flags": "--expose-gc",
  "dependencies": {
    "ssh2": "^0.5.2"
  },
  "webview": {
     "partitions": [
        {
          "name": "trusted",
          "accessible_resources": [ "<all_urls>" ]
        }
     ]
  }
}
